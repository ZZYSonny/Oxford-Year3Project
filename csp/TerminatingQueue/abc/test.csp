include "../common.csp"
include "../correct.csp"
include "queue.csp"
include "lin.csp"

Spec2Thread=Linearizers({T1,T2})

System1 = runWith(True,True,
  (enqueue(T1,A);enqueue(T1,B);enqueue(T1,C);dequeue(T1)) |||
  runP(dequeue(T2))
)
assert Spec2Thread [T= System1

System2 = runWith(True,True,
  (chaosP(enqueue(T1,A));enqueue(T1,B);chaosP(enqueue(T1,C))) |||
  runP(dequeue(T2))
)
assert Spec2Thread [T= System2

